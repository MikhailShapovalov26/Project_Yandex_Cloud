global:
  scrape_interval: 15s
  evaluation_interval: 15s


scrape_configs:
  - job_name: 'prometheus_metrics'
    scrape_interval: 5s
    static_configs:
      - targets: ['localhost:9090']
  - job_name: "node_exporter"
    scrape_interval: 5s
    static_configs:
       - targets: ['localhost:9100']

